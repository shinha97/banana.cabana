{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension provides the user with useful information for beach days!",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
	"default_title": "Beach Advisor"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}